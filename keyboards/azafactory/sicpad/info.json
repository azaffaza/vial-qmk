            "layout": [
                { "label": "Insert", "matrix": [0, 0], "x": 0, "y": 0 },
                { "label": "Home", "matrix": [0, 1], "x": 1, "y": 0 },
                { "label": "PgUp", "matrix": [0, 2], "x": 2, "y": 0 },
                { "label": "Delete", "matrix": [1, 0], "x": 0, "y": 1 },
                { "label": "End", "matrix": [1, 1], "x": 1, "y": 1 },
                { "label": "PgDn", "matrix": [1, 2], "x": 2, "y": 1 }
            ]
        }
    }
}