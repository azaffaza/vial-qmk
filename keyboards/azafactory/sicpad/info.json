{
    "manufacturer": "azafactory",
    "keyboard_name": "Sicpad",
    "maintainer": "azaffaza",
    "processor": "STM32F042",
    "bootloader": "stm32-dfu",
    "board": "GENERIC_STM32_F042X6",
	"build": {
		"debounce_type": "asym_eager_defer_pk"
    },
    "usb": {
        "vid": "0xFEED",
        "pid": "0x2402",
        "device_version": "1.0.0"
    },
    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "encoder": true,
        "extrakey": true,
        "mousekey": false,
        "nkro": true,
        "rgb_matrix": true
    },
    "encoder": {
        "rotary": [
            { "pin_a": "F0", "pin_b": "F1" }
        ]
    },
	"rgb_matrix": {
		"driver": "ws2812",
		"layout": [
			{ "flags": 4, "matrix": [1, 0], "x": 224, "y": 0 },
			{ "flags": 4, "matrix": [1, 1], "x": 112, "y": 0 },
			{ "flags": 4, "matrix": [1, 2], "x": 0, "y": 0 },
			{ "flags": 4, "matrix": [2, 0], "x": 224, "y": 64 },
			{ "flags": 4, "matrix": [2, 1], "x": 112, "y": 64 },
			{ "flags": 4, "matrix": [2, 2], "x": 0, "y": 64 }
		]
	},
    "diode_direction": "COL2ROW",
    "matrix_pins": {
        "direct": [
            [null, "A14", null],
            ["A2", "A3", "A4"],
            ["A7", "A6", "A5"]
        ]
    },
    "url": "https://github.com/azaffaza/azafactory",
    "layouts": {
        "LAYOUT": {
            "layout": [
                { "matrix": [0, 0], "x": 1, "y": 0 },
                { "label": "Insert", "matrix": [1, 0], "x": 0, "y": 1.25 },
                { "label": "Home", "matrix": [1, 1], "x": 1, "y": 1.25 },
                { "label": "PgUp", "matrix": [1, 2], "x": 2, "y": 1.25 },
                { "label": "Delete", "matrix": [2, 0], "x": 0, "y": 2.25 },
                { "label": "End", "matrix": [2, 1], "x": 1, "y": 2.25 },
                { "label": "PgDn", "matrix": [2, 2], "x": 2, "y": 2.25 }
            ]
        }
    }
}